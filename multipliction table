#include <iostream>

int main() {
    int limit;
    std::cout << "Enter the limit for the multiplication table: ";
    std::cin >> limit;

    std::cout << "\nMultiplication Table:\n";
    for (int i = 1; i <= limit; i++) {
        for (int j = 1; j <= limit; j++) {
            std::cout << i * j << "\t";
        }
        std::cout << "\n";
    }

    return 0;
}
