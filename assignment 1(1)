#include <iostream>
class DivisibleByNine {
private:
    int count;
    int sum;
public:
    DivisibleByNine() : count(0), sum(0) {}
    ~DivisibleByNine() {
        std::cout << "Number of integers divisible by 9: " << count << std::endl;
        std::cout << "Sum of integers divisible by 9: " << sum << std::endl;
    }
    void findAndSum() {
        for (int i = 100; i <= 200; ++i) {
            if (i % 9 == 0) {
                count++;
                sum += i;
            }
        }
    }
};
int main() {
    DivisibleByNine obj;
    obj.findAndSum();
    return 0;
