#include <iostream>
int main() {
    int n, first = 0, second = 1, next;
    std::cout << "Enter the number of terms: ";
    std::cin >> n;
    std::cout << "Fibonacci sequence: " << first << " " << second << " ";
    int i = 2;
    while (i < n) {
        next = first + second;
        first = second;
        second = next;
        std::cout << next << " ";
        i++;
    }
    return 0;
}
